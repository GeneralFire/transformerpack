#string STR_PCH_SLP_S0_SETTING_USB3_DBC                 #language en-US "USB3 DbC"
#string STR_PCH_SLP_S0_SETTING_XDP3                     #language en-US "XDP3"

#string STR_PCH_DEBUG_NO_CHANGE                         #language en-US "No Change"

