TOKEN
    Name  = "REBUILD_FSP_AND_BIOS"
    Value  = "0"
    Help  = "Main switch to enable RebuildFspBios support in Project"
    TokenType = Boolean
    TargetMAK = Yes
    Master = Yes
End

PATH
    Name  = "REBUILD_FSP_DIR"
End

MODULE
