    Parent  = "PLATFORM_DSC_INCLUDE_PATHS"
    InvokeOrder = AfterParent
End

ELINK
    Name  = "$(SB_CHIPSET_DIR)/SBGeneric.c"
    Parent  = "AMI_CSP_PEI_SOURCES"
    InvokeOrder = AfterParent
End

ELINK
    Name  = "$(SB_CHIPSET_DIR)/SBGeneric.c"
    Parent  = "AMI_CSP_DXE_SOURCES"
    InvokeOrder = AfterParent
End

