TOKEN
    Name  = "SB_SUPPORT"
    Value  = "1"
    Help  = "Main switch to enable Template - SouthBridge support in Project"
    TokenType = Boolean
    TargetMAK = Yes
    Master = Yes
End

PATH
    Name  = "SB_CHIPSET_DIR"
    Path  = "AmiCrbPkg/Chipset/SB"
End

PATH
    Name  = "SB_BOARD_DIR"
    Path  = "AmiCrbPkg/Board/SB"
End

MODULE
    Help  = "Includes SB.mak to Project"
    File  = "SB.mak"
End

ELINK
    Name  = "$(SB_CHIPSET_DIR)"
    Parent  = "PLATFORM_DSC_INCLUDE_PATHS"
    InvokeOrder = AfterParent
End

ELINK
